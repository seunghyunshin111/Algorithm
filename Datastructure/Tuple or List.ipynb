   ],
   "source": [
    "my_tuple = (1, 2, 3)\n",
    "\n",
    "try:\n",
    "    #여기에 인덱싱을 이용해서 Tuple의 값을 변경해봅시다.\n",
    "    my_tuple[2] = 5\n",
    "    \n",
    "except TypeError:\n",
    "    print(\"Tuple은 값을 변경할 수 없습니다.\")\n",
    "    \n",
    "try:\n",
    "    #여기에 .append()를 이용해서 Tuple의 값을 추가해봅시다.\n",
    "    my_tuple.append(6)\n",
    "    \n",
    "except AttributeError:\n",
    "    print(\"Tuple은 값을 추가할 수 없습니다.\")"
   ]
  }
 ],
