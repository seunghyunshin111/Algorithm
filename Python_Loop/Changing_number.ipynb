   "source": [
    "# 양수 출력 및 *10 출력\n",
    "\n",
    "x = [3, 6, 9, 20, -7, 5]\n",
    "\n",
    "for i in x:\n",
    "  if i < 0:\n",
    "    i = i * -1\n",
    "     \n",
    "  if i % 2 == 0:\n",
    "      i = i * 10\n",
    "  elif i % 2 != 0:\n",
    "      i = i * 10\n",
    "\n",
    "  print(i)"
   ]
  }
